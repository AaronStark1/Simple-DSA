class Solution {
    public List<Integer> findDuplicates(int[] nums) {
        int [] count = new int[nums.length +1];
        List<Integer> result = new ArrayList<>();
        for(int i = 0 ; i < nums.length ; i++){
            count[nums[i]]++;
        }

        for(int i = 0 ; i < count.length ; i++){
            if(count[i] == 2){
                result.add(i);
            }
        }
        return result;
    }
}
